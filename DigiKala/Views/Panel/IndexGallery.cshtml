@model IEnumerable<DigiKala.DataAccessLayer.Entities.ProductGallery>

@{
    Layout = null;
}


@foreach (var item in Model)
{
    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 container-gallery" style="margin-bottom: 10px">

        <img src="~/images/galleries/@item.Img" alt="تصویر پیدا نشد" class="img-responsive image-gallery" style="margin: auto" />
        <div class="overlay">
            <a href="/Panel/DeleteGallery/@item.Id">حذف شود؟</a>
        </div>

    </div>


}
